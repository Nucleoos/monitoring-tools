{
"name":"generic-splunk-http",

"description":"Pack to monitor splunk http web page",

"path":"services/",

"macros":{
         "_SPLUNKAPACHE":           {"type": "string",
                                     "description": "IP of apache server where splunk is. Used if splunk is behind a Apache auth",
                                    },

         "_SPLUNKURL":              {"type": "string",
                                     "description": "Splunk url on the webserver",
                                    },

         "_SPLUNKUSER":             {"type": "string",
                                     "description": "Splunk password",
                                    },

         "_SPLUNKPASSWORD":         {"type": "string",
                                     "description": "Splunk user password",
                                    },

         "_SPLUNKWARN":             {"type": "integer",
                                     "description": "Response time warning threshold",
                                    },

         "_SPLUNKCRIT":             {"type": "integer",
                                     "description": "Response time warning threshold",
                                    },

        }
}